<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- http://stackoverflow.com/questions/7568579/eclipsefindbugs-exclude-filter-files-doesnt-work -->
    <Match>
        <Or>
            <Class name="~.*.R"/>
            <Class name="~.*\.R\$.*"/>
            <Class name="~.*\.Manifest\$.*"/>
            <Class name="~.*\.DataBindingInfo.class" />
            <Class name="~.*\.android\.databinding\$.*" />
            <Class name="~com.android\$.*" />
            <Class name="~.*\.BR.class" />
        </Or>
    </Match>
</FindBugsFilter>